<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mafish.mafish">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReportActivity" />
        <activity android:name=".AddReportActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".FishShopActivity" />
        <activity android:name=".AddFishShopActivity" />
        <activity android:name=".DetailFishShopActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".DetailReportActivity" />
        <activity android:name=".InfoPriceFishActivity"></activity>
        <activity android:name=".DetailTunaFishShopActivity"></activity>
    </application>

</manifest>